{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://acid-respect-boulder-brothers.trycloudflare.com/api/:path*"
    },
    {
      "source": "/graphql",
      "destination": "https://acid-respect-boulder-brothers.trycloudflare.com/graphql"
    },
    {
      "source": "/((?!api|graphql|_next|static|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}
